{
	"name": "fast-files",
	"displayName": "Fast files",
	"description": "Generate files from your templates in one click !",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"publisher": "anthonyjeamme",
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/anthonyjeamme/fast-files.git"
	},
	"activationEvents": [
		"onCommand:fast-files.quick-generate",
		"onCommand:fast-files.custom-generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fast-files.quick-generate",
				"title": "⚡ Generate from default template"
			},
			{
				"command": "fast-files.custom-generate",
				"title": "⚙️ Generate files from template"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "fast-files.quick-generate",
					"group": "custom"
				},
				{
					"when": "explorerResourceIsFolder",
					"command": "fast-files.custom-generate",
					"group": "custom"
				}
			]
		}
	},
	"icon": "images/icon.png",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vsce": "^1.96.1",
		"vscode-test": "^1.4.1"
	}
}
